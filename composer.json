{
    "name": "yus-ham/yii2-resend",
    "description": "Yii 2 extension for [Resend](https://resend.com)",
    "keywords": ["yii2", "extension", "resend-php"],
    "type": "yii2-extension",
    "homepage": "https://github.com/yus-ham/yii2-resend",
    "license": "MIT",
    "authors": [
        {
            "name": "Yusup Hambali",
            "email": "sup.alpuket@gmail.com"
        }
    ],
    "require": {
        "php": ">8.0",
        "yiisoft/yii2": "~2.0",
        "resend/resend-php": "*"
    },
    "autoload": {
        "psr-4": {
            "yusham\\resend\\" : "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
